<odoo>
    <data>
        <record model="ir.ui.view" id="yuju_combos_config_form_view">
            <field name="name">yuju.combox.config.form.view</field>
            <field name="model">madkting.config</field>
            <field name="inherit_id" ref="madkting.madkting_config_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook" position="inside">
                    <field name="mrp_route" />
                    <field name="delete_old_bom" />
                    <field name="search_kit_by_sku" />                    
                    <field name="update_product_type_kits" />
                    <field name="product_type_for_kits" />  
                </xpath>                
            </field>
        </record>
    
    </data>
</odoo>

